{
    "theme_color": "#009e00",
    "background_color": "#ececf4",
    "display": "standalone",
    "orientation": "portrait",
    "scope": "/",
    "start_url": "/",
    "name": "FatLook – отчеты из FatSecret",
    "description": "Приложение для построения удобных отчетов питания из FatSecret",
    "short_name": "FatLook",
    "icons": [
        {
            "src": "/favicon/icon-192x192.png",
            "sizes": "192x192",
            "type": "image/png",
            "purpose": "any"
        },
        {
            "src": "/favicon/icon-512x512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        }
    ],
    "screenshots": [
        {
            "src": "/screens/1.png",
            "type": "image/png",
            "sizes": "425x943",
            "form_factor": "narrow"
        },
        {
            "src": "/screens/2.png",
            "type": "image/png",
            "sizes": "425x943",
            "form_factor": "narrow"
        },
        {
            "src": "/screens/3.png",
            "type": "image/png",
            "sizes": "425x943",
            "form_factor": "narrow"
        }
    ],
    "display_override": ["window-controls-overlay"],
    "protocol_handlers": [
        {
            "protocol": "web+fatlook",
            "url": "/?from=%s"
        }
    ],
    "shortcuts": [
        {
            "name": "Открыть FatLook",
            "url": "/"
        }
    ],
    "launch_handler": {
        "client_mode": "focus-existing"
    },
    "capture_links": "existing_client_event",
    "url_handlers": [
        {
            "origin": "https://fatlook.vercel.app"
        }
    ]
}
